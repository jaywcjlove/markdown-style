{
  "name": "@wcj/markdown-style",
  "version": "1.1.0",
  "description": "Integrate markdown styles into web components, Markdown CSS styles will not be conflicted.",
  "type": "module",
  "main": "lib/index.js",
  "module": "lib/index.js",
  "types": "lib/index.d.ts",
  "homepage": "https://jaywcjlove.github.io/markdown-style/",
  "funding": "https://jaywcjlove.github.io/#/sponsor",
  "repository": {
    "type": "git",
    "url": "https://github.com/jaywcjlove/markdown-style"
  },
  "license": "MIT",
  "scripts": {
    "prepare": "husky",
    "demo": "npx sgo --fallback demo.html",
    "build": "tsbb build",
    "watch": "tsbb watch"
  },
  "files": [
    "lib",
    "src"
  ],
  "keywords": [
    "web-component",
    "web-components",
    "markdown-style",
    "markdown",
    "web"
  ],
  "engines": {
    "node": ">=16.0.0"
  },
  "lint-staged": {
    "*.{css,md,json,prettierrc}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "devDependencies": {
    "husky": "^9.0.0",
    "lint-staged": "^15.0.0",
    "prettier": "^3.0.0",
    "tsbb": "^4.1.4"
  }
}
